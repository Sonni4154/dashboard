{
  "name": "marin-pest-control-backend",
  "version": "2.0.0",
  "description": "QuickBooks Backend API for Marin Pest Control Dashboard",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "dev": "npx tsx watch src/index.ts",
    "dev:with-services": "node start-dev.js --with-services",
    "build": "npx tsc",
    "start": "node dist/index.js",
    "start:token-refresh": "node dist/services/tokenRefresher.js",
    "start:sync": "node dist/services/syncService.js",
    "pm2:start": "pm2 start ecosystem.config.cjs",
    "pm2:stop": "pm2 stop ecosystem.config.cjs",
    "pm2:restart": "pm2 restart ecosystem.config.cjs",
    "pm2:delete": "pm2 delete ecosystem.config.cjs",
    "pm2:status": "pm2 status",
    "pm2:logs": "pm2 logs",
    "check": "tsc --noEmit",
    "lint": "eslint . --ext ts --report-unused-disable-directives --max-warnings 0",
    "test:webhook": "curl -X POST http://localhost:5000/api/webhook/quickbooks -H 'Content-Type: application/json' -d '{\"eventNotifications\":[{\"dataChangeEvent\":{\"entities\":[{\"name\":\"Customer\",\"id\":\"1\",\"operation\":\"Update\"}]}}]}'",
    "test:sync": "curl -X POST http://localhost:5000/api/sync",
    "test:sync-status": "curl http://localhost:5000/api/sync/status",
    "test:health": "curl http://localhost:5000/health",
    "test:api-health": "curl http://localhost:5000/api/health",
    "test:webhook-health": "curl http://localhost:5000/api/webhook/health",
    "test:all-endpoints": "npm run test:health && npm run test:api-health && npm run test:webhook-health && npm run test:sync-status",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "db:check": "drizzle-kit check",
    "logs:tail": "tail -f logs/combined.log",
    "logs:error": "tail -f logs/error.log",
    "logs:clear": "rm -f logs/*.log",
    "clean:dist": "rm -rf dist",
    "clean:logs": "rm -f logs/*.log",
    "clean:all": "npm run clean:dist && npm run clean:logs",
    "restart:dev": "npm run clean:all && npm run build && npm run dev",
    "deploy:check": "npm run check && npm run lint && npm run build",
    "deploy:full": "npm run deploy:check && npm run pm2:restart"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.75.0",
    "@types/pg": "^8.15.5",
    "axios": "^1.12.2",
    "bcrypt": "^6.0.0",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "drizzle-orm": "^0.36.4",
    "express": "^4.21.2",
    "express-rate-limit": "^8.1.0",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "jwks-client": "^2.1.0",
    "node-cron": "^4.2.1",
    "pg": "^8.16.3",
    "postgres": "^3.4.7",
    "winston": "^3.18.3"
  },
  "devDependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/node": "^22.10.2",
    "@typescript-eslint/eslint-plugin": "^8.18.1",
    "@typescript-eslint/parser": "^8.18.1",
    "drizzle-kit": "^0.29.1",
    "eslint": "^9.17.0",
    "googleapis": "^162.0.0",
    "open": "^10.2.0",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "quickbooks",
    "api",
    "backend",
    "marin",
    "pest-control",
    "drizzle",
    "supabase",
    "express"
  ],
  "author": "Spencer Reiser â€” Marin Pest Control",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Sonni4154/dashboard.git"
  }
}
