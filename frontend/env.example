# Marin Pest Control Dashboard - Frontend Environment Variables
# Copy this file to .env and fill in your actual values

# ===========================================
# API CONFIGURATION
# ===========================================
# Backend API base URL
VITE_API_BASE_URL=""
# Empty = use same domain with /api prefix (recommended)
# Or set to full URL like "http://localhost:5000" for development
# In production, leave empty to use same domain

# ===========================================
# SUPABASE CONFIGURATION
# ===========================================
# Supabase project configuration for frontend
VITE_SUPABASE_URL=https://jpzhrnuchnfmagcjlorc.supabase.co
VITE_SUPABASE_PUBLISHABLE_DEFAULT_KEY=sb_publishable_jViFA4A2JPObUqt-AM1O_g__l99_DIJ

# ===========================================
# DEVELOPMENT CONFIGURATION
# ===========================================
# Development mode flags
VITE_DEV_MODE="true"
VITE_MOCK_AUTH="true"
# Set to "false" in production

# ===========================================
# APPLICATION CONFIGURATION
# ===========================================
# App name and version
VITE_APP_NAME="Marin Pest Control Dashboard"
VITE_APP_VERSION="2.0.0"

# ===========================================
# FEATURE FLAGS
# ===========================================
# Enable/disable features
VITE_ENABLE_DEBUG_MODE="true"
VITE_ENABLE_ANALYTICS="false"
VITE_ENABLE_ERROR_REPORTING="false"

# ===========================================
# EXTERNAL SERVICES
# ===========================================
# Google Analytics (if enabled)
VITE_GA_TRACKING_ID=""
# Sentry error reporting (if enabled)
VITE_SENTRY_DSN=""

# ===========================================
# QUICKBOOKS INTEGRATION
# ===========================================
# QuickBooks integration settings
VITE_QBO_ENABLED="true"
VITE_QBO_SANDBOX_MODE="true"
# Set to "false" for production QuickBooks

# ===========================================
# UI CONFIGURATION
# ===========================================
# Theme and UI settings
VITE_DEFAULT_THEME="light"
VITE_ENABLE_DARK_MODE="true"
VITE_ITEMS_PER_PAGE="50"

# ===========================================
# CACHING CONFIGURATION
# ===========================================
# TanStack Query cache settings
VITE_QUERY_STALE_TIME="300000"
# 5 minutes in milliseconds
VITE_QUERY_CACHE_TIME="600000"
# 10 minutes in milliseconds
